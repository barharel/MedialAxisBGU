<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bar.medialaxisbgu" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"

            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PhaseOneActivity"
            android:label="@string/title_activity_main2" >
            <intent-filter>
                <action android:name="com.ran.myapplication.Main2Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShapeActivity"
            android:label="@string/title_activity_main22" >
            <intent-filter>
                <action android:name="com.ran.myapplication.Main22Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PhaseTwoActivity"
            android:label="@string/title_activity_phase_two" >
        </activity>
        <activity
            android:name=".BeginActivity"
            android:label="@string/title_activity_instructions" >
        </activity>
    </application>

</manifest>
